{
	"[javascript][javascriptreact][typescript][typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"editor.tabSize": 2,
	"eslint.enable": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"eslint.workingDirectories": [
		"src/*",
		"eslint-config/*"
	],
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"npm.exclude": [
		"**/src/**",
		"**/eslint-config/**"
	],
	"terminal.integrated.defaultProfile.osx": "zsh",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#96ebd0",
		"activityBar.background": "#96ebd0",
		"activityBar.foreground": "#15202b",
		"activityBar.inactiveForeground": "#15202b99",
		"activityBarBadge.background": "#c379e5",
		"activityBarBadge.foreground": "#15202b",
		"commandCenter.border": "#15202b99",
		"sash.hoverBorder": "#96ebd0",
		"statusBar.background": "#6be3bd",
		"statusBar.foreground": "#15202b",
		"statusBarItem.hoverBackground": "#40dbaa",
		"statusBarItem.remoteBackground": "#6be3bd",
		"statusBarItem.remoteForeground": "#15202b",
		"titleBar.activeBackground": "#6be3bd",
		"titleBar.activeForeground": "#15202b",
		"titleBar.inactiveBackground": "#6be3bd99",
		"titleBar.inactiveForeground": "#15202b99"
	},
}
